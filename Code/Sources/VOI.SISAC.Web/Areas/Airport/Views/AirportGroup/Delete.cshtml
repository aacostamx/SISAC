@model VOI.SISAC.Web.Models.VO.Airport.AirportGroupVO

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container">
    <div class="row">
        <p class="subtitle_h3_black">@Resource.AirportGroupTitle</p>
        <p class="subtitle_h3_ancillarie">@Resource.Delete</p>
    </div>

    @Html.Partial("_AlertView")
    <div class="form-horizontal">
        <div class="row">
            <div class="col-xs-12 col-sm-6 col-md-4 col-lg-4">
                @Html.LabelFor(model => model.AirportGroupCode, htmlAttributes: new { @class = "subtitle_h5 control-label" })
                @Html.EditorFor(model => model.AirportGroupCode, new { htmlAttributes = new { @class = "form-control specialInput", @readonly = "readonly" } })
            </div>
            <div class="col-xs-12 col-sm-6 col-md-4 col-lg-4">
                @Html.LabelFor(model => model.AirportGroupName, htmlAttributes: new { @class = "subtitle_h5 control-label" })
                @Html.EditorFor(model => model.AirportGroupName, new { htmlAttributes = new { @class = "form-control specialInput", @readonly = "readonly" } })
            </div>
        </div>
    </div>
</div>
<br />
<div class="container">
    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()
        <div class="row">
            <div>
                <br />
                <input onclick="location.href='@Url.Action("Index", "AirportGroup")'" type="reset" class="btnVolaris" value=@Resource.Back>
                <input type="submit" value=@Resource.Delete class="btnVolaris" />
            </div>
        </div>
    }
</div>
