
@{
    ViewBag.Title = "Unauthorized";
}

<div class="container">
    <div class="row">
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 centerText">
            <h2>@Resource.Unauthorized</h2>
            <a>@Resource.UnauthorizedMessage</a>
            <br />
        </div>
    </div>
    <div class="row">
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 centerText">
            <button id="btnBackToPage" type="button" class="btn btn-default" name="@Resource.Back" title="@Resource.Back" onclick="window.history.back();">
                <i class="fa fa-arrow-left fa-fw"></i>
                &nbsp;@Resource.Back
            </button>
        </div>
    </div>
</div>