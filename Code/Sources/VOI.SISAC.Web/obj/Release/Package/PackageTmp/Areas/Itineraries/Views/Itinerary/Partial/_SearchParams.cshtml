
<div id="divSearchItineary" class="panel-group" role="tablist" aria-multiselectable="true" hidden>
    <div class="panel panel-default">
        <div class="panel-heading" role="tab">
            <h4 class="panel-title subtitle_h6">
                <strong>@Resource.AdvanceSearch</strong>
            </h4>
        </div>
        <div class="row">
            <div id="colAirline" class="col-xs-12 col-sm-12 col-md-2 col-lg-2">
                <span class="subtitle_h5 control-label">
                    @Resource.AirlineCode
                </span>
                <span id="spanAirline" class="subtitle_h6_blue_block">
                    &nbsp;
                </span>
            </div>

            <div id="colFlightNumber" class="col-xs-12 col-sm-12 col-md-2 col-lg-2">
                <span class="subtitle_h5 control-label">
                    @Resource.FlightNumber
                </span>
                <span id="spanFlightNumber" class="subtitle_h6_blue_block">
                    &nbsp;
                </span>
            </div>

            <div id="colEquipmentNumber" class="col-xs-12 col-sm-12 col-md-2 col-lg-2">
                <span class="subtitle_h5 control-label">
                    @Resource.EquipmentNumber
                </span>
                <span id="spanEquipmentNumber" class="subtitle_h6_blue_block">
                    &nbsp;
                </span>
            </div>

            <div id="colDepartureStation" class="col-xs-12 col-sm-12 col-md-3 col-lg-3">
                <span class="subtitle_h5 control-label">
                    @Resource.DepartureStation
                </span>
                <span id="spanDepartureStation" class="subtitle_h6_blue_block">
                    &nbsp;
                </span>
            </div>

            <div id="colArrivalStation" class="col-xs-12 col-sm-12 col-md-3 col-lg-3">
                <span class="subtitle_h5 control-label">
                    @Resource.ArrivalStation
                </span>
                <span id="spanArrivalStation" class="subtitle_h6_blue_block">
                    &nbsp;
                </span>
            </div>
            <div id="colLocalizationStation" class="col-xs-12 col-sm-12 col-md-3 col-lg-3">
                <span class="subtitle_h5 control-label">
                    @Resource.LocalizationStation
                </span>
                <span id="spanLocalizationStation" class="subtitle_h6_blue_block">
                    &nbsp;
                </span>
            </div>
            <div id="colDepartureDateStart" class="col-xs-12 col-sm-12 col-md-2 col-lg-2">
                <span class="subtitle_h5 control-label">
                    @Resource.StartDate
                </span>
                <span id="spanDepartureDateStart" class="subtitle_h6_blue_block">
                    &nbsp;
                </span>
            </div>

            <div id="colDepartureDateEnd" class="col-xs-12 col-sm-12 col-md-2 col-lg-2">
                <span class="subtitle_h5 control-label">
                    @Resource.EndDate
                </span>
                <span id="spanDepartureDateEnd" class="subtitle_h6_blue_block">
                    &nbsp;
                </span>
            </div>
        </div>
    </div>
</div>