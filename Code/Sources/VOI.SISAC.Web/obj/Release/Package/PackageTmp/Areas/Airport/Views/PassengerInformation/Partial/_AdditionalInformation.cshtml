@model VOI.SISAC.Web.Models.VO.Airport.AdditionalPassengerInformationVO
<div id="add-info-modal" class="modal fade" role="dialog">
    <div class="modal-dialog">
        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="subtitle_h3_black">@Resource.AdditionalInformation</h4>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-xs-12 col-sm-12 col-md-1 col-lg-1"></div>
                    <div class="col-xs-12 col-sm-12 col-md-11 col-lg-11">
                        <div class="">
                            <span class="info subtitle_h5"><i class="fa fa-info-circle"></i>&nbsp;&nbsp;&nbsp;@Resource.ShowPreviousInformation</span>
                            <div class="tooltip-popup">
                                <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                                    <span><strong>No. de Pasajeros Totales</strong></span>
                                </div>
                                <br />
                                <br />
                                <div class="row">
                                    <div class="col-xs-12 col-sm-12 col-md-8 col-lg-8">
                                        <span>@Resource.PayTua:&nbsp;</span>
                                    </div>
                                    <div class="col-xs-12 col-sm-12 col-md-4 col-lg-4">
                                        <span id="pg-tua-pi"></span>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-xs-12 col-sm-12 col-md-8 col-lg-8">
                                        <span>@Resource.DiplomaticPassenger:&nbsp;&nbsp;</span>
                                    </div>
                                    <div class="col-xs-12 col-sm-12 col-md-4 col-lg-4">
                                        <span id="dip-pi"></span>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-xs-12 col-sm-12 col-md-8 col-lg-8">
                                        <span>@Resource.ExtraCrew:&nbsp;&nbsp;</span>
                                    </div>
                                    <div class="col-xs-12 col-sm-12 col-md-4 col-lg-4">
                                        <span id="ext-pi"></span>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-xs-12 col-sm-12 col-md-8 col-lg-8">
                                        <span>@Resource.Infant:&nbsp;&nbsp;</span>
                                    </div>
                                    <div class="col-xs-12 col-sm-12 col-md-4 col-lg-4">
                                        <span id="int-pi"></span>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-xs-12 col-sm-12 col-md-8 col-lg-8">
                                        <span>@Resource.TransitoryPassenger:&nbsp;&nbsp;</span>
                                    </div>
                                    <div class="col-xs-12 col-sm-12 col-md-4 col-lg-4">
                                        <span id="tra-pi"></span>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-xs-12 col-sm-12 col-md-8 col-lg-8">
                                        <span>@Resource.ConnectionPassenger:&nbsp;&nbsp;</span>
                                    </div>
                                    <div class="col-xs-12 col-sm-12 col-md-4 col-lg-4">
                                        <span id="con-pi"></span>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-xs-12 col-sm-12 col-md-8 col-lg-8">
                                        <span>@Resource.OtherExempt:&nbsp;&nbsp;</span>
                                    </div>
                                    <div class="col-xs-12 col-sm-12 col-md-4 col-lg-4">
                                        <span id="oth-pi"></span>
                                    </div>
                                </div>
                                <br />
                                <br />
                                <div class="row">
                                    <div class="col-xs-12 col-sm-12 col-md-8 col-lg-8">
                                        <strong><span>@Resource.Total:&nbsp;&nbsp;</span></strong>
                                    </div>
                                    <div class="col-xs-12 col-sm-12 col-md-4 col-lg-4">
                                        <strong><span id="tot-pi"></span></strong>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <br />
                        <div class="row">
                            <div class="col-xs-2 col-sm-2 col-md-4 col-lg-4 overflow_hidden">
                                <br />
                                
                            </div>
                            <div class="col-xs-2 col-sm-2 col-md-2 col-lg-2">
                                <span class="subtitle_h5 control-label">NAC</span>
                            </div>
                            <div class="col-xs-2 col-sm-2 col-md-2 col-lg-2">
                                <span class="subtitle_h5 control-label">INTER</span>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-xs-2 col-sm-2 col-md-4 col-lg-4 overflow_hidden">
                                <br />
                                <span class="subtitle_h5 control-label">@Resource.Adults</span>
                            </div>
                            <div class="col-xs-2 col-sm-2 col-md-2 col-lg-2">
                                <input value="@Model.AdultNational" class="form-control total-value" id="adu-nat" type="number" onkeypress="return commonFunctions.validarNumeros(event);" onkeyup="return commonFunctions.validarNumeros(event)" onchange="PassengerController.sumAdditionalInfo()" />
                            </div>
                            <div class="col-xs-2 col-sm-2 col-md-2 col-lg-2">
                                <input value="@Model.AdultInternational" class="form-control total-value" id="adu-int" type="number" onkeypress="return commonFunctions.validarNumeros(event);" onkeyup="return commonFunctions.validarNumeros(event)" onchange="PassengerController.sumAdditionalInfo()" />
                            </div>
                        </div>

                        <div class="row">
                            <br />
                            <div class="col-xs-2 col-sm-2 col-md-4 col-lg-4 overflow_hidden">
                                <span class="subtitle_h5 control-label">@Resource.Minor</span>
                            </div>
                            <div class="col-xs-2 col-sm-2 col-md-2 col-lg-2">
                                <input value="@Model.MinorNational" class="form-control total-value" id="mid-nat" type="number" onkeypress="return commonFunctions.validarNumeros(event);" onkeyup="return commonFunctions.validarNumeros(event)" onchange="PassengerController.sumAdditionalInfo()" />
                            </div>
                            <div class="col-xs-2 col-sm-2 col-md-2 col-lg-2">
                                <input value="@Model.MinorInternational" class="form-control total-value" id="mid-int" type="number" onkeypress="return commonFunctions.validarNumeros(event);" onkeyup="return commonFunctions.validarNumeros(event)" onchange="PassengerController.sumAdditionalInfo()" />
                            </div>
                        </div>
                        <br />
                        <div class="row">
                            <div class="col-xs-2 col-sm-2 col-md-4 col-lg-4 overflow_hidden total-label">
                                <span class="subtitle_h5 control-label"><strong>@Resource.PayTua</strong></span>
                            </div>
                            <div class="col-xs-2 col-sm-2 col-md-2 col-lg-2 total-value">
                                <span class="subtitle_h5 control-label" id="pay-tua-nat"></span>
                            </div>
                            <div class="col-xs-2 col-sm-2 col-md-2 col-lg-2 total-value">
                                <span class="subtitle_h5 control-label" id="pay-tua-int"></span>
                            </div>
                            <div class="col-xs-2 col-sm-2 col-md-2 col-lg-2 total-value">
                                <span class="subtitle_h5 control-label" id="pay-tua-tot"></span>
                            </div>
                        </div>
                        <br />
                        <div class="row">
                            <div class="col-xs-2 col-sm-2 col-md-4 col-lg-4 overflow_hidden">
                                <br />
                                <span class="subtitle_h5 control-label">@Resource.DiplomaticPassenger</span>
                            </div>
                            <div class="col-xs-2 col-sm-2 col-md-2 col-lg-2">
                                <input value="@Model.DiplomaticNational" class="form-control total-value" id="dip-nat" type="number" onkeypress="return commonFunctions.validarNumeros(event);" onkeyup="return commonFunctions.validarNumeros(event)" onchange="PassengerController.sumAdditionalInfo()" />
                            </div>
                            <div class="col-xs-2 col-sm-2 col-md-2 col-lg-2">
                                <input value="@Model.DiplomaticInternational" class="form-control total-value" id="dip-int" type="number" onkeypress="return commonFunctions.validarNumeros(event);" onkeyup="return commonFunctions.validarNumeros(event)" onchange="PassengerController.sumAdditionalInfo()" />
                            </div>
                            <div class="col-xs-2 col-sm-2 col-md-2 col-lg-2 total-value">
                                <br />
                                <span class="subtitle_h5 control-label" id="dip-tot"></span>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-xs-2 col-sm-2 col-md-4 col-lg-4 overflow_hidden">
                                <br />
                                <span class="subtitle_h5 control-label">@Resource.ExtraCrew</span>
                            </div>
                            <div class="col-xs-2 col-sm-2 col-md-2 col-lg-2">
                                <input value="@Model.CommissionNational" class="form-control total-value" id="ext-nat" type="number" onkeypress="return commonFunctions.validarNumeros(event);" onkeyup="return commonFunctions.validarNumeros(event)" onchange="PassengerController.sumAdditionalInfo()" />
                            </div>
                            <div class="col-xs-2 col-sm-2 col-md-2 col-lg-2">
                                <input value="@Model.CommissionInternational" class="form-control total-value" id="ext-int" type="number" onkeypress="return commonFunctions.validarNumeros(event);" onkeyup="return commonFunctions.validarNumeros(event)" onchange="PassengerController.sumAdditionalInfo()" />
                            </div>
                            <div class="col-xs-2 col-sm-2 col-md-2 col-lg-2 total-value">
                                <br />
                                <span class="subtitle_h5 control-label" id="ext-tot"></span>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-xs-2 col-sm-2 col-md-4 col-lg-4 overflow_hidden">
                                <br />
                                <span class="subtitle_h5 control-label">@Resource.Infant</span>
                            </div>
                            <div class="col-xs-2 col-sm-2 col-md-2 col-lg-2">
                                <input value="@Model.InfantNational" class="form-control total-value" id="inf-nat" type="number" onkeypress="return commonFunctions.validarNumeros(event);" onkeyup="return commonFunctions.validarNumeros(event)" onchange="PassengerController.sumAdditionalInfo()" />
                            </div>
                            <div class="col-xs-2 col-sm-2 col-md-2 col-lg-2">
                                <input value="@Model.InfantInternational" class="form-control total-value" id="inf-int" type="number" onkeypress="return commonFunctions.validarNumeros(event);" onkeyup="return commonFunctions.validarNumeros(event)" onchange="PassengerController.sumAdditionalInfo()" />
                            </div>
                            <div class="col-xs-2 col-sm-2 col-md-2 col-lg-2 total-value">
                                <br />
                                <span class="subtitle_h5 control-label" id="inf-tot"></span>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-xs-2 col-sm-2 col-md-4 col-lg-4 overflow_hidden">
                                <br />
                                <span class="subtitle_h5 control-label">@Resource.TransitoryPassenger</span>
                            </div>
                            <div class="col-xs-2 col-sm-2 col-md-2 col-lg-2">
                                <input value="@Model.TransitoryNational" class="form-control total-value" id="tran-nat" type="number" onkeypress="return commonFunctions.validarNumeros(event);" onkeyup="return commonFunctions.validarNumeros(event)" onchange="PassengerController.sumAdditionalInfo()" />
                            </div>
                            <div class="col-xs-2 col-sm-2 col-md-2 col-lg-2">
                                <input value="@Model.TransitoryInternational" class="form-control total-value" id="tran-int" type="number" onkeypress="return commonFunctions.validarNumeros(event);" onkeyup="return commonFunctions.validarNumeros(event)" onchange="PassengerController.sumAdditionalInfo()" />
                            </div>
                            <div class="col-xs-2 col-sm-2 col-md-2 col-lg-2 total-value">
                                <br />
                                <span class="subtitle_h5 control-label" id="tran-tot"></span>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-xs-2 col-sm-2 col-md-4 col-lg-4 overflow_hidden">
                                <br />
                                <span class="subtitle_h5 control-label">@Resource.ConnectionPassenger</span>
                            </div>
                            <div class="col-xs-2 col-sm-2 col-md-2 col-lg-2">
                                <input value="@Model.ConnectionNational" class="form-control total-value" id="con-nat" type="number" onkeypress="return commonFunctions.validarNumeros(event);" onkeyup="return commonFunctions.validarNumeros(event)" onchange="PassengerController.sumAdditionalInfo()" />
                            </div>
                            <div class="col-xs-2 col-sm-2 col-md-2 col-lg-2">
                                <input value="@Model.ConnectionInternational" class="form-control total-value" id="con-int" type="number" onkeypress="return commonFunctions.validarNumeros(event);" onkeyup="return commonFunctions.validarNumeros(event)" onchange="PassengerController.sumAdditionalInfo()" />
                            </div>
                            <div class="col-xs-2 col-sm-2 col-md-2 col-lg-2 total-value">
                                <br />
                                <span class="subtitle_h5 control-label" id="con-tot"></span>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-xs-2 col-sm-2 col-md-4 col-lg-4 overflow_hidden">
                                <br />
                                <span class="subtitle_h5 control-label">@Resource.OtherExempt</span>
                            </div>
                            <div class="col-xs-2 col-sm-2 col-md-2 col-lg-2">
                                <input value="@Model.OtherNational" class="form-control total-value" id="exempt-nat" type="number" onkeypress="return commonFunctions.validarNumeros(event);" onkeyup="return commonFunctions.validarNumeros(event)" onchange="PassengerController.sumAdditionalInfo()" />
                            </div>
                            <div class="col-xs-2 col-sm-2 col-md-2 col-lg-2">
                                <input value="@Model.OtherInternational" class="form-control total-value" id="exempt-int" type="number" onkeypress="return commonFunctions.validarNumeros(event);" onkeyup="return commonFunctions.validarNumeros(event)" onchange="PassengerController.sumAdditionalInfo()" />
                            </div>
                            <div class="col-xs-2 col-sm-2 col-md-2 col-lg-2 total-value">
                                <br />
                                <span class="subtitle_h5 control-label" id="exempt-tot"></span>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-xs-2 col-sm-2 col-md-4 col-lg-4 overflow_hidden"></div>
                            <div class="col-xs-2 col-sm-2 col-md-4 col-lg-4 overflow_hidden">
                                <br />
                                <span class="subtitle_h5 control-label"><strong>@Resource.Total</strong></span>
                            </div>
                            <div class="col-xs-2 col-sm-2 col-md-2 col-lg-2 total-value">
                                <br />
                                <span class="subtitle_h5 control-label" id="totals"></span>
                            </div>
                        </div>
                        <br />
                        <div class="row">
                            <div class="col-xs-2 col-sm-2 col-md-4 col-lg-4">
                                <span class="subtitle_h5 control-label" id="pax-dni">@Resource.PaxDni</span>
                            </div>
                            <div class="col-xs-2 col-sm-2 col-md-2 col-lg-2">
                                <input value="@Model.PaxDni" class="form-control total-value" id="pax-dni-val" type="number" onkeypress="return commonFunctions.validarNumeros(event);" onkeyup="return commonFunctions.validarNumeros(event)" />
                            </div>
                        </div>
                    </div>
                </div>

            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" name="save-btn" title="@Resource.Close" data-dismiss="modal">
                    <i class="fa fa-times fa-fw"></i>&nbsp;@Resource.Close
                </button>
                <button type="button" class="btn btn-default btn-green" name="save-btn" title="@Resource.Save" onclick="PassengerController.sumAdditionalInfo(); PassengerController.validateAddInfo()">
                    <i class="fa fa-floppy-o fa-fw"></i>&nbsp;@Resource.Save
                </button>
            </div>
        </div>
    </div>
</div>

