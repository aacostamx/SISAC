@model VOI.SISAC.Web.Models.VO.Airport.AirportScheduleVO

@{
    ViewBag.Title = @VOI.SISAC.Web.Resources.Resource.Delete;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container">
    <p class="subtitle_h3_black">@VOI.SISAC.Web.Resources.Resource.AirportScheduleTitle</p>
    <p class="subtitle_h3_ancillarie">@VOI.SISAC.Web.Resources.Resource.Delete</p>
</div>
<div class="container">
    @Html.Partial("_AlertView")
    <h3>@VOI.SISAC.Web.Resources.Resource.ConfirmDelete</h3>
    <div class="dl-horizontal">        
        <div class="row">
            <div class="col-md-4 col-sm-12 col-xs-12">
                @Html.LabelFor(model => model.StationCode, htmlAttributes: new { @class = "subtitle_h5 control-label" })
                @Html.EditorFor(model => model.StationCode, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
            </div>
            <div class="col-md-4 col-sm-12 col-xs-12">
                @Html.LabelFor(model => model.ScheduleTypeCode, htmlAttributes: new { @class = "subtitle_h5 control-label" })
                @Html.EditorFor(model => model.ScheduleTypeCode, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
            </div>
        </div>
        @Html.HiddenFor(model => model.AirportScheduleID)
        <div class="row">
            <div class="col-md-4 col-sm-12 col-xs-12">
                @Html.LabelFor(model => model.StartTimeSchedule, htmlAttributes: new { @class = "subtitle_h5 control-label" })
                @Html.EditorFor(model => model.StartTimeSchedule, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
            </div>
            <div class="col-md-4 col-sm-12 col-xs-12">
                @Html.LabelFor(model => model.EndTimeSchedule, htmlAttributes: new { @class = "subtitle_h5 control-label" })
                @Html.EditorFor(model => model.EndTimeSchedule, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
            </div>
        </div>

        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()

            <div class="row">
                <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                    <br />
                    <input type="reset" class="btnVolaris" value="@VOI.SISAC.Web.Resources.Resource.Back" onclick="@("location.href='" + @Url.Action("Index", "AirportSchedule") + "'")">
                    <input type="submit" value="@VOI.SISAC.Web.Resources.Resource.Delete" class="btnVolaris">
                    <br />
                </div>
            </div>
    }
    </div>
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}